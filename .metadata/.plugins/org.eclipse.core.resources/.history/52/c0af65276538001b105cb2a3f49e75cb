	<!-- 켈렉션 세터주입으로 넣는다 -->
	<bean class="ex05.collection.Custom">
	<!-- 켈렉션 String 타입 -->
		<property name="lists">
			<list>
				<value>홍길동</value>
				<value>이순신</value>
				<value>홍길자</value>
			</list>
		</property>
		
		<!-- 켈렉션  객체 타입 -->
		<property name="lists">
			<list>
				<bean class="ex05.collection.MemberVO">
				<property name="name" value="홍길동"/>
				<property name="id" value="kkk123"></property>
				</bean>MemberVO
				<bean class="ex05.collection.MemberVO">
				<property name="name" value="이순신"/>
				<property name="id" value="aaa123"></property>
				</bean>
			</list>
		</property>
		
		<!-- 컬랙션 맵 타입 -->
        <property name="maps">
            <map>
                <entry key="k1" value="홍길동"/>
                <entry key="k2">
                    <bean class="ex05.collection.MemberVO">
                        <property name="name" value="홍길동"/>
                        <property name="id" value="kkk123"/>
                    </bean>
                </entry>
            </map>
        </property>
		
		
		
		
	</bean>


